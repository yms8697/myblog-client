{
    "parser": "babel-eslint",
    "env": {
        "browser":true,
        "es6":true
    },
    "parserOptions":{
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "semi": [0],
        "react/jsx-filename-extension": [0],
        "object-curly-newline": [0],
        "react/prop-types": [0],
        "jsx-a11y/anchor-is-valid": [0],
        "class-methods-use-this": [0],
        "function-paren-newline": [0]
    }
}
